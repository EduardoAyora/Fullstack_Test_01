{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],

    "module": "ESNext",
    "moduleResolution": "Bundler",

    "jsx": "react-jsx",

    "strict": false,
    "skipLibCheck": true,

    "useDefineForClassFields": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src", "vite-env.d.ts"]
}
